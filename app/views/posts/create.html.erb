<h2>Posts</h2>
<% if @post.errors.any? %>
    <div id="error_explanation">
        <h2>
            <%= pluralize(@post.errors.count, "error") %>
            no permitieron ingresar el Post:
        </h2>
        <ul>
            <% @post.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
            <% end %>
        </ul>
    </div>
<% else%>
    <h4>Post ingresado con Ã©xito</h4>
<% end %>
<button type="button" class="btn btn-dark"><%= link_to "Volver", "/" %></button>




